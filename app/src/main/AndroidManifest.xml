<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yvs.moneysaving">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MoneySaving"
        android:usesCleartextTraffic="true">
        <activity android:name=".FAQItem1"></activity>
        <activity android:name=".FAQ" />
        <activity android:name=".UpdateHistory" />
        <activity android:name=".DetailHistory" />
        <activity android:name=".History" />
        <activity android:name=".NewTransactionIncome" />
        <activity android:name=".NewTransactionOutcome" />
        <activity android:name=".MyWallet">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>